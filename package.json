{
  "name": "prettier-standard-vscode",
  "displayName": "Prettier-Standard - JavaScript formatter",
  "description": "VS Code plugin for prettier + standard",
  "version": "1.0.2",
  "publisher": "numso",
  "author": "Dallin Osmun <@dallinosmun>",
  "galleryBanner": {
    "color": "#ffe04b",
    "theme": "light"
  },
  "homepage": "https://marketplace.visualstudio.com/items?itemName=numso.prettier-standard-vscode",
  "repository": {
    "type": "git",
    "url": "https://github.com/numso/prettier-standard-vscode.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/numso/prettier-standard-vscode/issues"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:mongo",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:json",
    "onLanguage:jsonc",
    "onLanguage:json5",
    "onLanguage:css",
    "onLanguage:postcss",
    "onLanguage:less",
    "onLanguage:scss",
    "onLanguage:handlebars",
    "onLanguage:graphql",
    "onLanguage:markdown",
    "onLanguage:mdx",
    "onLanguage:html",
    "onLanguage:vue",
    "onLanguage:yaml",
    "onLanguage:ansible",
    "onLanguage:home-assistant",
    "onLanguage:tailwindcss"
  ],
  "main": "./out/main",
  "dependencies": {
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "8.28.0",
    "prettier": "2.7.1",
    "typescript": "4.9.3",
    "vue-eslint-parser": "9.1.0"
  },
  "bin": "./src/prettier-standard-vscode.js"
}

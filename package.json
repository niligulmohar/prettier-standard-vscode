{
  "name": "prettier-standard-vscode",
  "displayName": "Prettier-Standard - JavaScript formatter",
  "description": "VS Code plugin for prettier + standard",
  "version": "0.9.2",
  "publisher": "numso",
  "author": "Dallin Osmun <@dallinosmun>",
  "galleryBanner": {
    "color": "#ffe04b",
    "theme": "light"
  },
  "homepage": "https://marketplace.visualstudio.com/items?itemName=numso.prettier-standard-vscode",
  "repository": {
    "type": "git",
    "url": "https://github.com/numso/prettier-standard-vscode.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/numso/prettier-standard-vscode/issues"
  },
  "engines": {
    "vscode": "^1.73.1"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:mongo",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:json",
    "onLanguage:jsonc",
    "onLanguage:json5",
    "onLanguage:css",
    "onLanguage:postcss",
    "onLanguage:less",
    "onLanguage:scss",
    "onLanguage:handlebars",
    "onLanguage:graphql",
    "onLanguage:markdown",
    "onLanguage:mdx",
    "onLanguage:html",
    "onLanguage:vue",
    "onLanguage:yaml",
    "onLanguage:ansible",
    "onLanguage:home-assistant",
    "onLanguage:tailwindcss"
  ],
  "main": "./out/main",
  "scripts": {
    "esbuild:base": "esbuild ./src/extension.js --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "build": "rm -rf ./out && yarn esbuild:base -- --minify && vsce package",
    "dev": "yarn esbuild:base -- --sourcemap --watch"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.1",
    "esbuild": "0.15.14"
  },
  "dependencies": {
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "8.28.0",
    "prettier": "2.7.1",
    "typescript": "4.9.3"
  }
}
